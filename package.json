{
  "name": "kliksutin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@sentry/browser": "^7.31.1",
    "@sentry/node": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "^27.5.2",
    "@types/node-cron": "^3.0.8",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@uiw/react-md-editor": "^3.23.3",
    "@vitejs/plugin-react": "^4.0.4",
    "axios": "^1.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "i18next": "^22.4.9",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "notistack": "^3.0.1",
    "openai": "^3.1.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-i18next": "^12.1.4",
    "react-markdown": "^8.0.5",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "remark-breaks": "^3.0.2",
    "sequelize": "^6.28.0",
    "umzug": "^3.2.1",
    "unfuck-utf8-headers-middleware": "^1.0.1",
    "uuid": "^9.0.0",
    "vite": "^4.4.8",
    "winston": "^3.8.2",
    "winston-gelf-transporter": "^1.0.2",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "docker compose up",
    "start:prod": "NODE_ENV=production node --loader ts-node/esm src/server/index.ts",
    "start:test": "NODE_ENV=test node --loader ts-node/esm src/server/index.ts",
    "start:dev": "NODE_ENV=development concurrently \"npm run start:dev:server\" \"npm run start:dev:front\"",
    "start:dev:front": "vite",
    "start:dev:server": "NODE_ENV=development ts-node-dev src/server/index.ts",
    "test": "npm run test:setup && npm run test:e2e",
    "test:setup": "docker-compose down && docker-compose -f docker-compose.ci.yml rm -vf && docker-compose -f docker-compose.ci.yml up -d --build",
    "test:e2e": "cypress run",
    "test:cypress": "cypress open",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write '*.{ts,tsx,json,css,md}'",
    "build": "DISABLE_ESLINT_PLUGIN=true vite build",
    "prepare": "husky install"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "prettier",
      "plugin:cypress/recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "react-hooks",
      "prettier",
      "cypress"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "*.{ts,tsx,json,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "concurrently": "^7.6.0",
    "cypress": "^12.3.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "proxy": "http://localhost:8000"
}
